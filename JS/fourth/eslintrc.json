{
"root": true,
"parser": "@typescript-eslint/parser",
"parserOptions": {
    "warnOnUnsupportedTypeScriptVersion": false,
    "sourceType": "module"
},
"env": {
    "browser": false,
    "node": true,
    "es6": true
},
"plugins": [
    "@typescript-eslint", "no-null", "import", "eslint-plugin-local", "simple-import-sort"
],
"ignorePatterns": [
    "**/node_modules/**",
    "/built/**",
    "/tests/**",
    "/lib/**",
    "/src/lib/*.generated.d.ts",
    "/scripts/**/*.js",
    "/scripts/**/*.d.*",
    "/internal/**",
    "/coverage/**"
]
}